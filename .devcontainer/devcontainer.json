{
  "name": "our_profile-app",
  "dockerComposeFile": "../docker/docker-compose.yml",
  "workspaceFolder": "/our_profile",
  "service": "our_profile-app",
  "forwardPorts": [3000],
  "remoteUser": "root",
  "postCreateCommand": "chmod u+x ./devcontainer/postCreateCommand.sh && sh ./devcontainer/postCreateCommand.sh",
  "postStartCommand": "chmod u+x ./devcontainer/postStartCommand.sh && sh ./devcontainer/postStartCommand.sh",

  "customizations": {
    "vscode": {
      "extensions": [
        "aki77.rails-db-schema",
        "castwide.solargraph",
        "kaiwood.endwise",
        "koichisasada.vscode-rdbg",
        "rebornix.ruby",
        "redhat.vscode-yaml",
        "mosapride.zenkaku",
        "vscode-icons-team.vscode-icons",
        "MS-CEINTL.vscode-language-pack-ja",
        "esbenp.prettier-vscode",
        "ecmel.vscode-html-css",
        "pranaygp.vscode-css-peek",
        "formulahendry.auto-rename-tag"
      ],
      "settings": {"../.vscode/settings.json": true}
    }
  }
}
